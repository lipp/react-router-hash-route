{
  "name": "react-router-hash-route",
  "version": "1.0.0",
  "description": "Enable hash / id based routes for react-router v4",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --presets=env,react --plugins=transform-object-rest-spread,transform-class-properties",
    "prepublish": "npm run build",
    "dev": "npm link && babel src -d lib --watch --presets=env,react --plugins=transform-object-rest-spread,transform-class-properties"
  },
  "files": [
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lipp/react-router-hash-route.git"
  },
  "keywords": [
    "react",
    "react-router",
    "link",
    "hash",
    "hash-link",
    "scroll"
  ],
  "author": "Gerhard Preuss <gelipp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lipp/react-router-hash-route/issues"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-router-dom": ">=4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "react": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
